#page{
  padding:10rpx;
  display:flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.calendar{
  text-align: center;
  width:180rpx;
 
  height:60rpx;
  line-height: 60rpx;
  box-sizing: border-box;
}

#content{
  width:96%;
  box-sizing: border-box;
  margin-left:2%;
  padding:10rpx;
  margin-top:40rpx;
  box-shadow: 10rpx 10rpx 5rpx rgb(248, 246, 246);
  border-radius: 10rpx;
}

.hisList{
  padding-left:15rpx;
  display:flex;
  flex-direction: column;
  margin-top:10rpx;
  margin-bottom: 20rpx;
}
#datePlace{
  width:70%;
  height:60rpx;
  margin-top:20rpx;
  display:flex;
  justify-content: space-around;
}
.btn{
  width:150rpx;
  height:60rpx;
  line-height: 60rpx;
  font-size:30rpx;
  background-color:rgb(36, 197, 49);
  color:rgb(243, 239, 239);
}
.date{
  
  font-size:32rpx;
  color:rgb(155, 152, 152);
}
.title{
  font-size:34rpx;
  margin-top:10rpx; 
}

van-datetime-picker{
  width:600rpx;
}